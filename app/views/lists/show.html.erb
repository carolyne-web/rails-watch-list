<%# # A user can see the details of a given list and its name (see list/id) %>
<h1> <%= @list.name %> </h1>

<%@list.bookmarks.each do |bookmark|%>
<h2> <%= bookmark.movie.title %> </h2>
<p> <%= bookmark.movie.overview %></p>
<p> <%= bookmark.movie.poster_url %> </p>
<p><%= bookmark.movie.rating %> </p>
<% end %>

<%= link_to "home", lists_path %>
<%= link_to "Create new bookmark", new_list_bookmark_path(@list) %>
